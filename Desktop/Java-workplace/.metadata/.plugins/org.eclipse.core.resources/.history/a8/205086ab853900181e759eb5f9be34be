package junkCode;

class Animal1 {
		private String name;  
	    private int id; 
	    public Animal1(String myName, int myid) { 
	        name = myName; 
	        id = myid;
	    } 
	    public void eat(){ 
	        System.out.println(name+"正在吃"); 
	    }
	    public void sleep(){
	        System.out.println(name+"正在睡");
	    }
	    public void introduction() { 
	        System.out.println("大家好！我是"+ id + "号" + name + "."); 
	    } 
}
class Penguin extends Animal1{
	public Penguin(String myName,int myid) {
		super(myName,myid);
	}
	void bark() {
		System.out.println("jiji");
	}
}
class Mouse extends Animal1{
	public Mouse(String myName,int myid) {
		super(myName,myid);
	}
	void bark() {
		System.out.println("zhizhi");
	}
}
public class Animal1TestDrive{
	Penguin a=new Penguin("ass",1);
	Mouse b=new Mouse("hole",2);
}
